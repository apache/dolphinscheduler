{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start Web",
      "type": "shell",
      "command": ". ~/.nvm/nvm.sh && nvm use 16.20.2 && npm run dev",
      "options": {
        "cwd": "${workspaceFolder}/dolphinscheduler-ui" // 替换为你的前端文件夹路径
      },
      "problemMatcher": [
        {
          "owner": "custom",
          "pattern": [
            {
              "regexp": ".",
              "file": 1,
              "location": 2,
              "message": 3
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".",
            "endsPattern": "."
          }
        }
      ],
      "isBackground": true,
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      }
    }
  ]
}