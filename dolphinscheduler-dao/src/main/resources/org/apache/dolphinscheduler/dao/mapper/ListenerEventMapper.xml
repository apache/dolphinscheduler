<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.apache.dolphinscheduler.dao.mapper.ListenerEventMapper">
    <insert id="batchInsert">
        insert into t_ds_listener_event (title, sign, content, post_status, event_type, log, plugin_instance_id)
        values
        <foreach collection="listenerEvents" item="listenerEvent" separator=",">
            (#{listenerEvent.title},#{listenerEvent.sign},#{listenerEvent.content},#{listenerEvent.postStatus},
            #{listenerEvent.eventType},#{listenerEvent.log},#{listenerEvent.pluginInstanceId})
        </foreach>
    </insert>

</mapper>
