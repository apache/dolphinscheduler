            {
              "configs":[
                "dolphin-common/resource.storage.type"
              ],
              "if": "${dolphin-common/resource.storage.type} === HDFS || ${dolphin-common/resource.storage.type} === S3",
              "then": {
                "property_value_attributes": {
                  "visible": true
                }
